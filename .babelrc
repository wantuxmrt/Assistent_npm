{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-transform-runtime",
    [
      "babel-plugin-module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "@components": "./src/components",
          "@assets": "./src/assets",
          "@utils": "./src/utils",
          "@services": "./src/services",
          "@contexts": "./src/contexts",
          "@hooks": "./src/hooks",
          "@store": "./src/store",
          "@pages": "./src/pages"
        }
      }
    ]
  ]
}